  procedures :
   -
     name : LoadBannerInfo
     procedure_id : 1458396552625172030
     type_code : 14
     code : |1+
      // Summary: Change the information for the banner
      // Syntax:
      //LoadBannerInfo (<nSalesID> is int)
      //
      // Parameters:
      //	nSalesID (integer): ID of the sales whose information must be retrieved
      // Return value:
      // 	None
      //
      
      procedure LoadBannerInfo(nSalesID is int)
      
      
      HReadSeekFirst(sales,SalesID, nSalesID)
      if HFound(sales) then
      	
      	STC_PromoSalesID = nSalesID
      	IMG_Bar =  Sales.Photo
      	STC_DestinationBanner = Sales.Destination 
      	STC_SalesBanner = Sales.SalesName 
      	STC_Reduc = HTMLToText(Sales.Discount)
      	
      end
      
     type : 458752
