     name : _CREATEFORMVALUE
     internal_properties : CAAAAAgAAAB7Wd+prIZRHtc0GN71AmWeyn9mhuTxpn0Nhx7WaEiyy1HzYR78E2vXbvgmRV/ysLwXGpgY7B/NrETnuplIilggR/uP+C8L3GLSt50dOiIceHFQo9DlYLgCXvW9b0Di0GoAv9SFDFa9Hcc=
     procedure_id : 1458396561224152746
     type_code : 14
     code : |1-
      // Summary: Create a hidden form control
      // Syntax:
      //[ <Result> = ] _CREATEFORMVALUE (<sName>, <vValue> [, <bOnlyIfDifferentDefaultValue>])
      //
      // Parameters:
      //	sName: Name of the control
      //	vValue: Value of the control
      //	bOnlyIfDifferentDefaultValue (default value=0): True if the HTML code must be generated only if the value differs from the default value
      // Return value:
      // 	string: HTML string
      
      procedure private _CREATEFORMVALUE(sName, local vValue, bOnlyIfDifferentFromDefaultValue = False)
      	
      	if bOnlyIfDifferentFromDefaultValue then
      		if vValue in ("", 0) then
      			return ""
      		end
      	end
      
      return StringBuild("<input type=""hidden"" name=""%1"" value=""%2"">", sName, vValue)		
