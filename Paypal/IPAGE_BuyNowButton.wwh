      procedure public SetCurrency(nCurrency)
      
      	switch nCurrency
      		case eCZK : gsCurrencyCode = "CZK"
      		case eDKK : gsCurrencyCode = "DKK"
      		case eEUR : gsCurrencyCode = "EUR"
      		case eGBP : gsCurrencyCode = "GBP"
      		case eHUF : gsCurrencyCode = "HUF"
      		case ePLN : gsCurrencyCode = "SCH"
      		case eSEK : gsCurrencyCode = "SEK"
      		case eUSD : gsCurrencyCode = "USD"			
      		case eJPY : gsCurrencyCode = "JPY"
      	end
      
      	_HTML()
       
